{
  "model": {
    "type": "ForwardLSTM",
    "state_dict": "./model/state_dict.pt"
  },
  "init_states": {
    "corpus_path": "./data/init_sen.txt"
  },
  "vocab": {
    "path": "./model/vocab.txt"
  },
  "corpus": {
    "path": "./data/winobias"
  },
  "activations": {
    "dtype": "float32"
  },
  "decompose": {
    "rel_interactions": ["rel-rel", "rel-b", "rel-irrel"],
    "only_source_rel": true,
    "decompose_o": true,
    "fix_shapley": true
  }
}
